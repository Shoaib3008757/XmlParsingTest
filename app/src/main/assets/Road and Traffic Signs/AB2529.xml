<QF id="AB2529" topic="Road and Traffic Signs" category="C&amp;M" ni_exempt="no">
	<question>
		<xref>DES s6, HC p102, KYTS p91</xref>
		<text>What does this sign mean?</text>
		<graphic>AB2529.png</graphic>
		<prompt>Mark one answer</prompt>
		<voice id="AB2529-1"/>
	<explanation>
		<text>Temporary restrictions on motorways are shown on signs which have flashing amber lights. At the end of the restriction you will see this sign without any flashing lights.</text>
		<voice id="AB2529-2"/>
	</explanation>
	</question>
	<answers>
		<answer correct="no">
			<text>End of motorway</text>
			<graphic></graphic>
		</answer>
		<answer correct="yes">
			<text>End of restriction</text>
			<graphic></graphic>
		</answer>
		<answer correct="no">
			<text>Lane ends ahead</text>
			<graphic></graphic>
		</answer>
		<answer correct="no">
			<text>Free recovery ends</text>
			<graphic></graphic>
		</answer>
	</answers>

</QF>
