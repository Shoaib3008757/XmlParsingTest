<QF id="AB2766" topic="Road and Traffic Signs" category="C&amp;M" ni_exempt="no">
	<question>
		<xref>DES s6, HC p108, KYTS p11</xref>
		<text>What does this sign mean?</text>
		<graphic>AB2766.png</graphic>
		<prompt>Mark one answer</prompt>
		<voice id="AB2766-1"/>
	<explanation>
		<text>This sign may be at the end of a dual carriageway or a one-way street. It is there to warn you of oncoming traffic.</text>
		<voice id="AB2766-2"/>
	</explanation>
	</question>
	<answers>
		<answer correct="no">
			<text>Two-way traffic ahead across a one-way road</text>
			<graphic></graphic>
		</answer>
		<answer correct="no">
			<text>Traffic approaching you has priority</text>
			<graphic></graphic>
		</answer>
		<answer correct="yes">
			<text>Two-way traffic straight ahead</text>
			<graphic></graphic>
		</answer>
		<answer correct="no">
			<text>Motorway contraflow system ahead</text>
			<graphic></graphic>
		</answer>
	</answers>

</QF>
